.list{
  padding: 20rpx 30rpx 0 30rpx;
  height: 196rpx;
  display: -webkit-flex; /* Safari ,Webkit内核的浏览器，必须加上-webkit前缀。*/  
  display: flex; 
  flex-direction: row; 
  border-bottom: 1rpx solid #f2f2f2;
}
.list1{
  width: 223rpx;
  height: 80%;
}
.list image{
  width: 100%;
  height: 100%;
}
.list2{
  font-size: 34rpx;
  margin-left: 20rpx;
}
.qian{
  color: #f62f30;
  position: absolute;
  right: 30rpx;
}
.shuliang{
  color: #666666;
  margin-top: 20rpx;
  font-size: 28rpx;
}
.shifu{
  padding: 30rpx;
  font-size: 24rpx;
  color: #999999;
  text-align: right;
}
.shifu span{
  font-size: 36rpx;
  color: #f62f30;
}
.biao{
   display: -webkit-flex; /* Safari ,Webkit内核的浏览器，必须加上-webkit前缀。*/  
  display: flex; 
  flex-direction: row; 
  align-items: center;
  padding: 30rpx;
  border-top: 20rpx solid #f2f2f2;
  font-size: 34rpx;
}
.biao input{
  margin-left: 30rpx;
  font-size: 30rpx;
}
.biao image{
  width: 50rpx;
  height: 43rpx;
  margin-right: 10rpx;
}
.zhifu{
  position: absolute;
  right: 30rpx;
}
.beizhu{
  border-top: 20rpx solid #f2f2f2;
}
.beizhu1{
  padding: 30rpx;
  font-size: 34rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
.beizhu2{
  padding: 30rpx;
}
.beizhu2 textarea{
  height: 200rpx;
}
.foot{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: right;
  background-color: #535353;
  color: white;
}
.tijiao{
  color: white;
  background-color: #f62f30;
  float: right
}
.foot text{
  height: 80rpx;
  line-height: 80rpx;
  display: inline-block;
  padding: 0 20rpx;
}
.heji{
  margin-right: 20rpx;
}